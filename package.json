{
  "name": "cronnaclimba",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "http-server ./ -c-1 --port 3000 --cors -H '{\"Cache-Control\": \"no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0\"}'",
    "dev": "concurrently \"npm run watch\" \"npm run serve\""
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "http-server": "^14.1.1",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "lucide": "^0.294.0"
  }
} 